                         ]]></sql>
            <!--PT_ETA_SYSTEM-->
            <sql><![CDATA[
                         SELECT P.*
                          FROM PT_ETA_SYSTEM P,TEMP_PT_SYSTEM T
                         WHERE P.SYSTEM_ID=T.PID;
                         ]]></sql>

            <!--PT_ETA_ACCESS-->
            <sql><![CDATA[
                         SELECT P.*
                          FROM PT_ETA_ACCESS P,  TEMP_PT_POI R
