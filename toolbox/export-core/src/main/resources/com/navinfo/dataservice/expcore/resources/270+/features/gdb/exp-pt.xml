            <!--//////////////////////////////////////////公交公司和公交系统////////////////////////////////////////////////////////////-->
            <!--PT_COMPANY-->
            <sql><![CDATA[
                         SELECT P.*
                          FROM PT_COMPANY P,TEMP_PT_COMPANY T
                         WHERE P.COMPANY_ID = T.PID ;
                         ]]></sql>

            <!--PT_SYSTEM-->
            <sql><![CDATA[
                         SELECT P.*
                          FROM PT_SYSTEM P,TEMP_PT_SYSTEM T WHERE P.SYSTEM_ID =T.PID;
                         ]]></sql>
            <!--//////////////////////////////////////////公交深度信息////////////////////////////////////////////////////////////-->
            <!--PT_ETA_COMPANY-->
            <sql><![CDATA[
                         SELECT P.*
                          FROM PT_ETA_COMPANY P,TEMP_PT_COMPANY T
