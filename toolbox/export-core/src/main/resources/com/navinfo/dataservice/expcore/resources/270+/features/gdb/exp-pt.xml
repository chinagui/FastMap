            <!--PT_STRAND_SCHEDULE-->
            <sql><![CDATA[
                         SELECT P.*
                          FROM PT_STRAND_SCHEDULE P,TEMP_PT_STRAND T
                         WHERE  P.STRAND_PID=t.PID;
